package com.phone;

public class Phone {
	
	private String os;
	private int ram;
	private String processor;
	private double screeSize;
	private int battery;
	
	
	public Phone(String os, int ram, String processor, double screeSize, int battery) {
		super();
		this.os = os;
		this.ram = ram;
		this.processor = processor;
		this.screeSize = screeSize;
		this.battery = battery;
	}
	
	
	

}
